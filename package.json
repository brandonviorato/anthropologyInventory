{
    "name": "anthropology-inventory",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "install": "npm-run-all install:*",
        "install:frontend": "cd frontend && npm install",
        "install:backend": "cd api && npm install",
        "dev": "npm-run-all -p dev:*",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd api && npm run dev"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
